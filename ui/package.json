{
  "name": "kleos-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clear-build": "rm -rf dist",
    "local-copy-config": "cp config.qa.js config.js",
    "copy-config": "cp setup.template.js dist/config.js",
    "dev": "npm run local-copy-config && vite",
    "build": "npm run clear-build && tsc && vite build && npm run copy-config",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint --fix \"src/**/?*.{ts,tsx,js,jsx}\"",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "eslint-check": "eslint \"**/?*.{ts,tsx,js,jsx}\"",
    "prettier-check": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "checkJiraID": "./jiraIDChecker.sh",
    "prepare": "husky install",
    "eslint-check-quiet": "yarn eslint-check --quiet"
  },
  "dependencies": {
    "@navi/dark-knight": "^1.0.13",
    "@navi/web-ui": "^1.58.15",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.11.0",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "emojione": "^4.5.0",
    "lottie-react": "^2.4.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-lottie": "^1.2.4",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react18-json-view": "^0.2.6",
    "redux-logger": "^3.0.6",
    "sass": "^1.69.5",
    "victory": "^36.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-lottie": "^1.2.10",
    "@types/react-redux": "^7.1.33",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-prefix-selector": "^1.16.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "yarn pretty"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix"
    ]
  }
}
